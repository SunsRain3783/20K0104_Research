<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/root_view"
    tools:context=".higashi_1f">

    <ImageView
        android:id="@+id/Image"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/higashi1f" />

    <ImageView
        android:id="@+id/usericon"
        android:layout_width="50dp"
        android:layout_height="65dp"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.516"
        app:srcCompat="@drawable/usericon" />

    <Button
        android:id="@+id/buttonMultUse"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:text="マルチユースホール"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.464" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0">

        <Button
            android:id="@+id/button5f"
            android:layout_width="88dp"
            android:layout_height="40dp"
            android:text="5F" />

        <Button
            android:id="@+id/button4f"
            android:layout_width="88dp"
            android:layout_height="40dp"
            android:text="4F" />

        <Button
            android:id="@+id/button3f"
            android:layout_width="88dp"
            android:layout_height="40dp"
            android:text="3F" />

        <Button
            android:id="@+id/button2f"
            android:layout_width="88dp"
            android:layout_height="40dp"
            android:text="2F" />

        <Button
            android:id="@+id/button1f"
            android:layout_width="88dp"
            android:layout_height="40dp"
            android:text="1F" />

        <Button
            android:id="@+id/buttonb1"
            android:layout_width="88dp"
            android:layout_height="40dp"
            android:text="B1" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="200dp"
        android:layout_height="43dp"
        android:gravity="center"
        android:text="東館 -1F-"
        android:textSize="20dp"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />

    <include layout="@layout/common_layout" />

</androidx.constraintlayout.widget.ConstraintLayout>